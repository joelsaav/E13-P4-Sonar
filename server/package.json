{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "cd .. && ./server/node_modules/.bin/prisma generate --schema ./server/prisma/schema.prisma && cd server && tsc",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "start": "node dist/server.js",
    "check": "tsc --noEmit",
    "check:watch": "tsc --noEmit --watch",
    "prisma:generate": "cd .. && ./server/node_modules/.bin/prisma generate --schema ./server/prisma/schema.prisma",
    "prisma:migrate": "cd .. && ./server/node_modules/.bin/prisma migrate dev --schema ./server/prisma/schema.prisma",
    "prisma:studio": "prisma studio",
    "prisma:notifications": "cd .. && ./server/node_modules/.bin/prisma migrate dev --name notifications --schema ./server/prisma/schema.prisma",
    "prettier": "prettier --write .",
    "coverage": "vitest run --coverage --coverage.include '**/src/*'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ai-sdk/google": "^2.0.45",
    "@prisma/client": "^6.18.0",
    "ai": "^5.0.108",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.11",
    "socket.io": "^4.8.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.1",
    "@types/nodemailer": "^7.0.4",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.12",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "google-auth-library": "^10.5.0",
    "prettier": "^3.6.2",
    "prisma": "^6.18.0",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.5"
  }
}
